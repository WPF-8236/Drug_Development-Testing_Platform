<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.WPF.dao.MessageDao">
    <select id="selectMessageBym_mark" resultType="Message">
        select *
        from ddtp_message
        where m_mark = #{m_mark}
    </select>
</mapper>